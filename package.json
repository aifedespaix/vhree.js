{
  "name": "vhree.js",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": [
    "./src/animations/builtins.ts"
  ],
  "peerDependencies": {
    "vue": ">=3.3",
    "three": ">=0.180.0"
  },
  "scripts": {
    "dev": "bunx vite",
    "preview": "bunx vite preview",
    "build": "rm -rf dist && bunx vite build && bunx vue-tsc --emitDeclarationOnly --declaration --declarationMap -p tsconfig.json",
    "typecheck": "bunx vue-tsc --noEmit",
    "dev:example": "bunx vite",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "^1.0.0-alpha.3",
    "@types/three": "^0.180.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "histoire": "^1.0.0-alpha.3",
    "three": "^0.180.0",
    "typescript": "~5.8.3",
    "vite": "npm:rolldown-vite@7.1.12",
    "vitepress": "^2.0.0-alpha.12",
    "vue": "^3.5.21",
    "vue-tsc": "^3.0.7"
  },
  "dependencies": {
    "@vueuse/core": "^13.9.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.12"
  }
}
